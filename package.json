{
  "name": "better-validator",
  "version": "2.1.4",
  "description": "Second generation object or request validator for node.js",
  "homepage": "https://bitbucket.org/gregbacchus/better-validator/overview",
  "keywords": [
    "validate",
    "validator",
    "validation",
    "express",
