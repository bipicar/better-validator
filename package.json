    "check",
    "test",
    "request-validation",
    "request-validator",
    "koa",
    "koajs",
    "koa.js",
    "typescript",
    "node",
    "node.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/gregbacchus/better-validator.git"
  },
  "scripts": {
    "test": "node_modules/.bin/jasmine",
    "lint": "node_modules/.bin/tslint --project tsconfig.json -t stylish",
