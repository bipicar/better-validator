    "body-parser": "^1.15.2",
    "del": "~3",
    "express": "~4",
    "istanbul": "~0.4",
    "jasmine": "~2",
    "koa": "~1",
    "supertest": "~3",
    "tslint": "~5",
    "typescript": "~2"
